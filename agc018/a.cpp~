#include <iostream>
#include <vector>
#include <algorithm>
#include <cstring>
#include <cmath>
#include <cstdlib>

#define MOD 1000000007
#define ll long long

using namespace std;
ll N,K;
vector<ll> A;

ll db[1000000001]={0};

void renew(ll ma){
  for(ll i = )
}

int main(){
  ll inp;
  
  cin.tie(0);
  ios::sync_with_stdio(false);

  cin >> N >> K;
  for(ll i = 0 ; i<N ; i++){
    cin >> inp;
    A.push_back(inp);
    db[inp]=1;
  }

  sort(A.begin(), A.end());
  if(K > A[A.size()-1]){
    cout << "IMPOSSIBLE" << endl;
    return 0;
  }

  
  
  return 0;
}
