#include <iostream>
#include <algorithm>
#include <cmath>
#include <vector>

using namespace std;


int main(){

  vector<long long> v;
  v.push_back(3);
  v.push_back(1);
  v.push_back(5);
  v.push_back(5);
  v.push_back(4);
  vector<long long>::iterator result;
  
  result = max_element(v.begin(), v.end());
  cout << "max element at: " << result[0] << '\n';
  
  
  return 0;
}
